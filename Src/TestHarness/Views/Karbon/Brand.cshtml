@using TestHarness.Models
@inherits KarbonView<Brand>

@Html.Md(Model.CurrentPage.Body)

<p>BRAND PAGE - @Model.CurrentPage.Name - @Model.CurrentPage.Slug - @Model.CurrentPage.Depth</p>
<ul>
    @foreach (var child in Model.CurrentPage.Children<Product>(x => x.IsVisible()))
    {
        <li>@child.Name</li>
    }
</ul>